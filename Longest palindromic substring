def longest_Palindromic_substring(N):
    sathwik=[]
    Balu=""
    for i in range(len(N)):
        for j in range(i+1,len(N)+1):
            Praneeth=N[i:j]
            if Praneeth==Praneeth[::-1]:
                sathwik.append(Praneeth)
    for j in sathwik:
        if len(j)>len(Balu):
            Balu=j
    return Balu
N=input()
print(longest_Palindromic_substring(N))


