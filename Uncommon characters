#User function Template for python3

class Solution:
    # Function to find uncommon characters between two strings.
    def uncommonChars(self, s1, s2):
        sathwik=set()
        Praneeth=[]
        for i in s1:
            if i not in s2 and i not in sathwik:
                sathwik.add(i)
                Praneeth.append(i)
        for k in s2:
            if k not in s1 and k not in sathwik:
                sathwik.add(k)
                Praneeth.append(k)
        Praneeth.sort()
        return "".join(Praneeth)
                
