
words = ["listen", "silent", "enlist", "hello"]

anagram_pairs = []


for i in range(len(words)):
    for j in range(i + 1, len(words)):
        word1 = words[i]
        word2 = words[j]
        

        if sorted(word1) == sorted(word2):
            anagram_pairs.append((word1, word2))

print("Anagram Pairs:", anagram_pairs)
