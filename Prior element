A = [10]

count1=0
for i in range(1,len(A)):
    sathwik=A[i]
    Praneeth=A[:i]
    P=True
    for yh in Praneeth:
        if sathwik>yh:
            P=True
        else:
            P=False
            break
    if P:
        count1=count1+1
if len(A)>=1:
    count1=count1+1
    print(count1)
else:
    print(count1)

