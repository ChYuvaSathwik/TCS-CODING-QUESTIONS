def sathwik(R,L):
    M=[]
    C=[]
    FGU=[]
    for i in range(1,R+1):
        if R%i==0:
            M.append(i)
    for j in range(1,L+1):
        if L%j==0:
            C.append(j)
    for w in M:
        if w in C:
            FGU.append(w)
    return max(FGU)
R,L=map(int,input().split())
print(sathwik(R,L))
