def count_substring_distinct(N):
    count1=0
    Balu=[]
    for i in range(len(N)):
        for j in range(i+1,len(N)+1):
            sathwik=N[i:j]
            if len(sathwik)==len(set(sathwik)) and sathwik not in Balu:
                Balu.append(sathwik)
                count1=count1+1
    return count1
N=input()
print(count_substring_distinct(N))
