def largest_prime(N):
    sathwik=[]
    for i in range(2,N):
        M=True
        for j in range(2,int(i**0.5)+1):
            if i%j==0:
                M=False
                break
        if M:
            sathwik.append(i)
    return max(sathwik)
N=int(input())
print(largest_prime(N))



