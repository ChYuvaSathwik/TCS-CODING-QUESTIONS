def largest_prime(N):
    B = []

    
    for i in range(N-1, 1, -1):  
        is_prime = True
        for j in range(2, int(i**0.5) + 1):
            if i % j == 0:
                is_prime = False
                break
        if is_prime:
            B.append(i)

    # Return the 4th largest prime (index 3)
    if len(B) >= 4:
        return B[3]
    else:
        return None 

# Input
N = int(input("Enter N: "))
result = largest_prime(N)

if result:
    print("4th largest prime less than N is:", result)
else:
    print("There are fewer than 4 primes less than N.")
