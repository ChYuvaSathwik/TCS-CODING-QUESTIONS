from bisect import bisect_right

class Solution:
    def countElements(self, a, b, n, query, q):
        b.sort()  # Sort b once
        X = []
        for i in query:
            # bisect_right returns index where a[i] would fit in b to keep it sorted
            # All elements before this index are <= a[i]
            count1 = bisect_right(b, a[i])
            X.append(count1)
        return X


